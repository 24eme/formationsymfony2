{% extends  'base.html.twig' %}
{% block body %}

{{ form_start(form, {
    attr: {novalidate : 'novalidate'}
  })
}}

<table >
<tr>
  <td>{{ form_row(form.date) }}</td>
  <td>{{ form_row(form.libelle) }}</td>
  <td>{{ form_row(form.equipeA) }}</td>
  <td>{{ form_row(form.equipeB) }}</td>
